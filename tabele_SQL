-- Firma stomatologiczna 

CREATE SCHEMA Firma;

CREATE TABLE Firma.Pracownik(
	IDPracownika VARCHAR(6) UNIQUE NOT NULL PRIMARY KEY,
	ImieLekarza VARCHAR(20) NOT NULL,
	NazwiskoLekarza VARCHAR(40) NOT NULL
);

CREATE TABLE Firma.Pacjent(
	IDPacjenta VARCHAR(6) NOT NULL PRIMARY KEY,
	ImiePacjenta VARCHAR(30) NOT NULL,
	NazwiskoPacjenta VARCHAR(50) NOT NULL
); 

CREATE TABLE Firma.Zabieg(
	IDZabiegu VARCHAR(5) UNIQUE NOT NULL PRIMARY KEY,
	NazwaZabiegu VARCHAR(30)
);

CREATE TABLE Firma.Wizyta(
	IDWizyty int UNIQUE NOT NULL PRIMARY KEY,
  DataWizyty DATE NOT NULL,
	GodzinaWizyty TIME NOT NULL,
  
	IDPracownika VARCHAR(6), REFERENCES Firma.Pracownik(IDPracownika),
	IDPacjenta VARCHAR(6)  REFERENCES Firma.Pacjent(IDPacjenta),
	IDZabiegu VARCHAR(5) REFERENCES Firma.Zabieg(IDZabiegu),
);


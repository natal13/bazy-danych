-- tabelka druga - Zamowienia 

CREATE SCHEMA Zamowienia 

CREATE TABLE Zamowienia.Produkt(
IDProduktu varchar(4) UNIQUE NOT NULL PRIMARY KEY,
NazwaProduktu varchar(40),
CenaBrutto DOUBLE(6), 
CenaNetto DOUBLE(6)
);

CREATE TABLE Zamowienia.Dostawca(
	IdDostawcy varchar(4) UNIQUE NOT NULL PRIMARY KEY,
	Dostawca varchar(40),
  MiejscowoscDostawcy VARCHAR(30)
  KodPocztowy VARCHAR(5)
	Ulica varchar(30),
	NrBudynku varchar(5)
);

CREATE TABLE Zamowienia.Usluga(
IDUslugi INT PRIMARY KEY UNIQUE NOT NULL,
IDProduktu VARCHAR(4) REFERENCES Zamowienia.Produkt(IdProduktu),
IDDostawcy VARCHAR(4) REFERENCES Zamowienia.Dostawca(IdDostawcy)
);

